.cord-box {
  background-color: #ffffff;
  border-radius: 20rpx;
  font-size: 32rpx;
  color: #282b32;
  padding: 24rpx;
  text-align: center;
}
.flex {
  display: flex;
  position: relative;
}
.cord-header {
  display: flex;
  justify-content: space-between;
  padding-bottom: 16rpx;
  /* padding: 0 23rpx; */
}
.cord-content {
  display: flex;
  align-items: center;
  justify-content: space-between;
  /* margin-top: 16rpx; */
}
.cord-text {
  color: #6d7275;
  font-size: 24rpx;
  font-weight: 400;
  font-family: PingFang SC, PingFang SC-Regular;
  margin-top: 10rpx;
}
.cord-num {
  font-family: DIN Alternate, DIN Alternate-Bold;
  font-size: 32rpx;
  color: #282b32;
  font-weight: 700;
  display: flex;
  justify-content: center;
}
image {
  width: 24rpx;
  height: 24rpx;
}
.box_tit {
  font-weight: 500;
  font-size: 24rpx;
  color: #282b32;
}
.box_tit text {
  font-weight: 300;
  margin-left: 6rpx;
  font-size: 18rpx;
}
.imgs_30 {
  width: 26rpx;
  height: 26rpx;
}
.align-conter {
  align-items: center;
}